import math
import random
R = float(input('Ieavdiet riņķa līnijas rādiusu:'))
9
print(R)
print('Riņķa līnijas garums:', '%.2f'%(2*math.pi*R))
print('Riņķa līnijas laukums:', '%.2f'%(math.pi * pow(R,2)))
